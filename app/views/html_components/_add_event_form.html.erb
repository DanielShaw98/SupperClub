<%= simple_form_for(@user, @event) do |f| %>
  <%= f.input :venue %>
  <%= f.input :menu, placeholder: "List your courses here" %>
  <%= f.input :cuisine %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :capacity, as: :integer %>
  <%= f.input :date, as: :date %>
  <%= f.submit %>
<% end %>
