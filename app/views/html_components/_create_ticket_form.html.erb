<%= simple_form_for [@user, @event, @ticket] do |f| %>
  <%= f.input :start_time, as: :string,
              input_html: { data: { controller: "timepickr" } } %>
  <%= f.input :end_time, as: :string,
              input_html: { data: { controller: "timepickr" } } %>
  <%= f.input :guests, label: "Number of guests"  %>
  <%= f.submit %>
<% end %>
